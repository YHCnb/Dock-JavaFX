<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="root" prefHeight="400" prefWidth="600" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yingtai.dock.SettingController">
   <children>
      <HBox fx:id="hBox" prefHeight="400" prefWidth="600">
         <children>
            <VBox fx:id="leftVBox" prefHeight="400" prefWidth="200">
               <children>
                  <Button fx:id="button1" mnemonicParsing="false" onAction="#showButton1Content" prefHeight="40.0" prefWidth="160" text="外观" />
                  <Button fx:id="button2" mnemonicParsing="false" onAction="#showButton2Content" prefHeight="40.0" prefWidth="160" text="图标" />
                  <Button fx:id="button3" mnemonicParsing="false" onAction="#showButton3Content" prefHeight="40.0" prefWidth="160" text="Dock栏" />
                  <Button fx:id="button4" mnemonicParsing="false" onAction="#showButton4Content"  prefHeight="40.0" prefWidth="160" text="时间与日期" />
                  <Button fx:id="button5" mnemonicParsing="false" onAction="#showButton5Content" prefHeight="40.0" prefWidth="160" text="关于" />
               </children>
            </VBox>
            <ScrollPane fx:id="rightScrollPane" hbarPolicy="NEVER" prefWidth="400">
               <content>
                  <VBox fx:id="rightVbox" prefWidth="400" />
               </content>
            </ScrollPane>
         </children>
      </HBox>
   </children>
</StackPane>
